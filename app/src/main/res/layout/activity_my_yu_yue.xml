<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/shenblue"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".activity.MyYuYueActivity">
    <LinearLayout
        android:id="@+id/aaaaa"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/dp_24"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_height="wrap_content">
        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/goupiaojilu_fanhui"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_16"
            android:background="@drawable/back" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_marginLeft="@dimen/dp_104"
            android:text="我的预约"
            android:textColor="@color/bai"
            />
    </LinearLayout>
<androidx.recyclerview.widget.RecyclerView
    android:layout_below="@id/aaaaa"
    android:id="@+id/yuyue_rlv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"></androidx.recyclerview.widget.RecyclerView>
<include layout="@layout/layout_include"
    android:visibility="gone"
    />
</RelativeLayout>